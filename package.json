{
  "name": "@adobe/helix-collab-service",
  "version": "0.0.1",
  "private": true,
  "description": "Helix Collab Service",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon",
    "test": "c8 mocha  -i -g 'Post-Deploy'",
    "test-postdeploy": "mocha -g 'Post-Deploy'",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "build": "hedy -v",
    "deploy": "hedy -v --deploy --test",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CI_BUILD_NUM -l ci --cleanup-ci 24h",
    "prepare": "husky"
  },
  "wsk": {
    "nodeVersion": 20,
    "target": "aws",
    "name": "helix-services/collab-service@${version}",
    "testUrl": "/_status_check/healthcheck.json",
    "memory": 2048,
    "apiId": "eynvwoxb7l",
    "fastlyServiceId!important": "",
    "awsRole!important": "arn:aws:iam::282898975672:role/helix-role-collab-test",
    "__awsAttachAuthorizer": "helix-token-authorizer_v2",
    "adapterFile": "./src/adapter.js",
    "handler": "index.handler",
    "timeout": 900000,
    "params-file": "secrets.json"
  },
  "mocha": {
    "spec": "test/**/*.test.js",
    "_require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-collab-service"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@adobe/fetch": "4.2.3",
    "@adobe/helix-shared-storage": "1.8.0",
    "@adobe/helix-shared-wrap": "2.0.2",
    "@adobe/helix-status": "10.1.5",
    "@aws-sdk/client-apigatewaymanagementapi": "3.908.0",
    "@aws-sdk/client-dynamodb": "3.908.0",
    "@aws-sdk/client-lambda": "3.910.0",
    "@aws-sdk/client-s3": "3.922.0",
    "@aws-sdk/lib-dynamodb": "3.908.0",
    "hast-util-from-html": "^2.0.3",
    "hast-util-select": "^6.0.4",
    "lib0": "0.2.114",
    "prosemirror-model": "^1.25.4",
    "prosemirror-schema-list": "^1.5.1",
    "prosemirror-tables": "^1.8.1",
    "ws": "8.18.3",
    "y-protocols": "1.0.6",
    "yjs": "13.6.27"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.10",
    "@adobe/helix-admin-support": "3.3.21",
    "@adobe/helix-deploy": "13.1.14",
    "@adobe/helix-universal": "5.2.3",
    "@adobe/helix-universal-devserver": "1.1.140",
    "@adobe/semantic-release-coralogix": "1.1.39",
    "@adobe/y-websocket": "git://github.com/adobe-rnd/y-websocket.git#3.0.0-base64",
    "@eslint/config-helpers": "0.3.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "c8": "10.1.3",
    "dotenv": "17.2.2",
    "eslint": "9.4.0",
    "husky": "9.1.7",
    "jsdom": "^27.0.1",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.1.6",
    "mocha": "11.7.2",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.5.6",
    "nodemon": "3.1.10",
    "prosemirror-state": "^1.4.4",
    "prosemirror-view": "^1.41.3",
    "semantic-release": "24.2.7",
    "semantic-release-slack-bot": "4.0.2",
    "y-prosemirror": "^1.3.7"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "nodemonConfig": {
    "exec": "node  --inspect ./test/dev/server.mjs",
    "watch": [
      ".env",
      "."
    ],
    "ext": ".js, .cjs, .ejs, .css"
  }
}
